{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "search-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/searchForm/SearchFormWebPart.js",
          "manifest": "./src/webparts/searchForm/SearchFormWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
      "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    },
      "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "globalName": "bootstrap"
    }
  },
  "localizedResources": {
    "SearchFormWebPartStrings": "lib/webparts/searchForm/loc/{locale}.js"
  }
}
